{
  "name": "playwright-saucedemo",
  "version": "1.0.0",
  "description": "Playwright E2E tests for Saucedemo using TypeScript",
  "main": "index.js",
  "scripts": {
    "postinstall": "playwright install --with-deps",
    "test": "playwright test",
    "test:ci": "playwright test --reporter=list,junit",
    "test:headed": "playwright test --headed",
    "report": "playwright show-report"
  },
  "keywords": [
    "playwright",
    "e2e-tests",
    "saucedemo",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.1"
  }
}
